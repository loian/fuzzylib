[System]
Name='NegationTest'
Type='mamdani'
Version=2.0
NumInputs=2
NumOutputs=1
NumRules=4
AndMethod='min'
OrMethod='max'
ImpMethod='min'
AggMethod='max'
DefuzzMethod='centroid'

[Input1]
Name='Temperature'
Range=[0 40]
NumMFs=2
MF1='Cold':'trapmf',[0 0 10 20]
MF2='Hot':'trapmf',[20 30 40 40]

[Input2]
Name='Humidity'
Range=[0 100]
NumMFs=2
MF1='Dry':'trapmf',[0 0 30 50]
MF2='Wet':'trapmf',[50 70 100 100]

[Output1]
Name='FanSpeed'
Range=[0 100]
NumMFs=3
MF1='Low':'trimf',[0 0 50]
MF2='Medium':'trimf',[25 50 75]
MF3='High':'trimf',[50 100 100]

[Rules]
# IF Temperature is NOT Cold AND Humidity is Dry THEN FanSpeed is Low
-1 1, 1 (1.0) : 1
# IF Temperature is Hot AND Humidity is NOT Dry THEN FanSpeed is High
2 -1, 3 (1.0) : 1
# IF Temperature is NOT Cold OR Humidity is Wet THEN FanSpeed is Medium
-1 2, 2 (0.8) : 2
# IF Temperature is Cold AND Humidity is Wet THEN FanSpeed is Medium
1 2, 2 (1.0) : 1
